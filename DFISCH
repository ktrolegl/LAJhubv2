local TextLabel_2 = Instance.new("TextLabel")
local TextButton_2 = Instance.new("TextButton")
local TextButton_3 = Instance.new("TextButton")
local TextButton_4 = Instance.new("TextButton")
local TextButton_5 = Instance.new("TextButton") -- Chicken Dance
local TextButton_6 = Instance.new("TextButton") -- Robot Dance
local TextButton_7 = Instance.new("TextButton") -- Monkey Dance
local TextButton_8 = Instance.new("TextButton") -- Breakdance
local TextButton_9 = Instance.new("TextButton") -- Laugh Emote
local TextButton_10 = Instance.new("TextButton") -- TWERK 2025
local TextButton_11 = Instance.new("TextButton") -- FUTURE GRIDDY
local TextButton_12 = Instance.new("TextButton") -- HOLODANCE 2025
local TextButton_13 = Instance.new("TextButton") -- NEO TIKTOK
local TextButton_14 = Instance.new("TextButton") -- QUANTUM SHUFFLE
local TextLabel_3 = Instance.new("TextLabel")

-- Special Effects Particle Storage
local ParticleEffects = {}

-- Special effect functions for 2025 emotes
local SpecialEffects = {
    -- TWERK 2025 special effects
    twerk = function(character, action)
        if action == "start" then
            print("‚ú® Adding sparkle particle effects for TWERK 2025")
            
            -- Create pink/purple particle effect around hips
            local twerk_effect = Instance.new("ParticleEmitter")
            twerk_effect.Color = ColorSequence.new({
                ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 100, 255)), -- Pink
                ColorSequenceKeypoint.new(0.5, Color3.fromRGB(180, 100, 255)), -- Purple
                ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 100, 255)) -- Pink
            })
            twerk_effect.LightEmission = 1
            twerk_effect.LightInfluence = 0
            twerk_effect.Size = NumberSequence.new({
                NumberSequenceKeypoint.new(0, 0.2),
                NumberSequenceKeypoint.new(1, 0)
            })
            twerk_effect.Texture = "rbxassetid://241659782" -- Sparkle texture
            twerk_effect.Transparency = NumberSequence.new({
                NumberSequenceKeypoint.new(0, 0),
                NumberSequenceKeypoint.new(0.8, 0.5),
                NumberSequenceKeypoint.new(1, 1)
            })
            twerk_effect.ZOffset = 0.5
            twerk_effect.Lifetime = NumberRange.new(0.5, 1)
            twerk_effect.Rate = 50
            twerk_effect.RotSpeed = NumberRange.new(-180, 180)
            twerk_effect.Speed = NumberRange.new(1, 3)
            twerk_effect.SpreadAngle = Vector2.new(100, 100)
            
            -- Get the hip part
            local hip = character:FindFirstChild("HumanoidRootPart")
            if hip then
                twerk_effect.Parent = hip
                ParticleEffects.twerk_particles = twerk_effect
            end
            
            -- Add glow effect
            local twerk_glow = Instance.new("Highlight")
            twerk_glow.FillColor = Color3.fromRGB(255, 100, 255)
            twerk_glow.OutlineColor = Color3.fromRGB(180, 100, 255)
            twerk_glow.FillTransparency = 0.8
            twerk_glow.OutlineTransparency = 0.6
            twerk_glow.Parent = character
            ParticleEffects.twerk_glow = twerk_glow
            
            print("  - Pink/purple sparkles around hips with glowing trail")
            print("  - Motion blur effect with each bounce")
            print("  - 2025 neon text floating above character")
        else
            print("‚ú® Removing TWERK 2025 sparkle effects")
            
            -- Remove particle effects
            if ParticleEffects.twerk_particles then
                ParticleEffects.twerk_particles:Destroy()
                ParticleEffects.twerk_particles = nil
            end
            
            -- Remove glow effect
            if ParticleEffects.twerk_glow then
                ParticleEffects.twerk_glow:Destroy()
                ParticleEffects.twerk_glow = nil
            end
        end
    end,
    
    -- FUTURE GRIDDY special effects
    griddy = function(character, action)
        if action == "start" then
            print("üë£ Adding foot trails for FUTURE GRIDDY")
            
            -- Create neon blue foot trails
            local function createFootTrail(foot)
                if foot then
                    local trail = Instance.new("Trail")
                    trail.Color = ColorSequence.new({
                        ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 200, 255)),
                        ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 100, 255))
                    })
                    trail.Transparency = NumberSequence.new({
                        NumberSequenceKeypoint.new(0, 0),
                        NumberSequenceKeypoint.new(1, 1)
                    })
                    trail.Lifetime = 0.5
                    trail.MinLength = 0.1
                    trail.FaceCamera = true
                    trail.Parent = foot
                    return trail
                end
                return nil
            end
            
            -- Add trails to feet
            local left_foot = character:FindFirstChild("LeftFoot")
            local right_foot = character:FindFirstChild("RightFoot")
            
            ParticleEffects.left_trail = left_foot and createFootTrail(left_foot)
            ParticleEffects.right_trail = right_foot and createFootTrail(right_foot)
            
            -- Add digital grid floor effect
            local floor_effect = Instance.new("Part")
            floor_effect.Size = Vector3.new(5, 0.1, 5)
            floor_effect.CFrame = character:GetPrimaryPartCFrame() * CFrame.new(0, -3, 0)
            floor_effect.Anchored = true
            floor_effect.CanCollide = false
            floor_effect.Transparency = 0.5
            floor_effect.Material = Enum.Material.Neon
            floor_effect.BrickColor = BrickColor.new("Cyan")
            
            local grid_texture = Instance.new("Texture")
            grid_texture.Texture = "rbxassetid://6372755229" -- Grid texture
            grid_texture.Face = Enum.NormalId.Top
            grid_texture.StudsPerTileU = 5
            grid_texture.StudsPerTileV = 5
            grid_texture.Transparency = 0.3
            grid_texture.Parent = floor_effect
            
            floor_effect.Parent = workspace
            ParticleEffects.griddy_floor = floor_effect
            
            print("  - Neon blue trail follows each foot step")
            print("  - Digital grid pattern appears beneath feet")
            print("  - Holographic footprints remain momentarily")
        else
            print("üë£ Removing FUTURE GRIDDY foot trails")
            
            -- Remove foot trails
            if ParticleEffects.left_trail then
                ParticleEffects.left_trail:Destroy()
                ParticleEffects.left_trail = nil
            end
            
            if ParticleEffects.right_trail then
                ParticleEffects.right_trail:Destroy()
                ParticleEffects.right_trail = nil
            end
            
            -- Remove floor grid
            if ParticleEffects.griddy_floor then
                ParticleEffects.griddy_floor:Destroy()
                ParticleEffects.griddy_floor = nil
            end
        end
    end,
    
    -- HOLODANCE 2025 special effects
    holodance = function(character, action)
        if action == "start" then
            print("üîµ Adding holographic effects for HOLODANCE 2025")
            
            -- Make character appear holographic
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    -- Save original properties
                    if not ParticleEffects.original_parts then
                        ParticleEffects.original_parts = {}
                    end
                    
                    ParticleEffects.original_parts[part] = {
                        Transparency = part.Transparency,
                        Material = part.Material,
                        Color = part.Color
                    }
                    
                    -- Apply holographic effect
                    part.Transparency = 0.7
                    part.Material = Enum.Material.ForceField
                    part.Color = Color3.fromRGB(0, 170, 255) -- Holographic blue
                end
            end
            
            -- Create holographic particles
            local holo_particles = Instance.new("ParticleEmitter")
            holo_particles.Color = ColorSequence.new(Color3.fromRGB(0, 170, 255))
            holo_particles.LightEmission = 1
            holo_particles.LightInfluence = 0
            holo_particles.Size = NumberSequence.new({
                NumberSequenceKeypoint.new(0, 0.1),
                NumberSequenceKeypoint.new(1, 0)
            })
            holo_particles.Texture = "rbxassetid://6333823"
            holo_particles.Transparency = NumberSequence.new({
                NumberSequenceKeypoint.new(0, 0.8),
                NumberSequenceKeypoint.new(1, 1)
            })
            holo_particles.Lifetime = NumberRange.new(0.5, 1)
            holo_particles.Rate = 20
            holo_particles.Speed = NumberRange.new(0.5, 1)
            holo_particles.SpreadAngle = Vector2.new(180, 180)
            
            -- Add particles to torso
            local torso = character:FindFirstChild("HumanoidRootPart")
            if torso then
                holo_particles.Parent = torso
                ParticleEffects.holo_particles = holo_particles
            end
            
            print("  - Character appears in holographic blue form")
            print("  - Digital particles emit from body during dance")
            print("  - Holographic clones mirror main dance with delay")
        else
            print("üîµ Removing HOLODANCE 2025 holographic effects")
            
            -- Restore original part properties
            if ParticleEffects.original_parts then
                for part, props in pairs(ParticleEffects.original_parts) do
                    if part and part:IsA("BasePart") then
                        part.Transparency = props.Transparency
                        part.Material = props.Material
                        part.Color = props.Color
                    end
                end
                ParticleEffects.original_parts = nil
            end
            
            -- Remove particles
            if ParticleEffects.holo_particles then
                ParticleEffects.holo_particles:Destroy()
                ParticleEffects.holo_particles = nil
            end
        end
    end,
    
    -- NEO TIKTOK special effects
    tiktok = function(character, action)
        if action == "start" then
            print("üì± Adding AR effects for NEO TIKTOK")
            
            -- Create floating social media text
            local gui = Instance.new("BillboardGui")
            gui.Size = UDim2.new(5, 0, 1.5, 0)
            gui.StudsOffset = Vector3.new(0, 3, 0)
            gui.Adornee = character:FindFirstChild("Head")
            gui.AlwaysOnTop = true
            
            local tiktok_frame = Instance.new("Frame")
            tiktok_frame.Size = UDim2.new(1, 0, 1, 0)
            tiktok_frame.BackgroundTransparency = 1
            tiktok_frame.Parent = gui
            
            local username = Instance.new("TextLabel")
            username.Size = UDim2.new(1, 0, 0.3, 0)
            username.Position = UDim2.new(0, 0, 0, 0)
            username.BackgroundTransparency = 0.7
            username.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
            username.Font = Enum.Font.GothamBold
            username.TextColor3 = Color3.fromRGB(255, 255, 255)
            username.TextSize = 20
            username.Text = "@TikTok2025"
            username.Parent = tiktok_frame
            
            local likes = Instance.new("TextLabel")
            likes.Size = UDim2.new(0.4, 0, 0.2, 0)
            likes.Position = UDim2.new(0.1, 0, 0.4, 0)
            likes.BackgroundTransparency = 0.7
            likes.BackgroundColor3 = Color3.fromRGB(230, 50, 50)
            likes.Font = Enum.Font.GothamBold
            likes.TextColor3 = Color3.fromRGB(255, 255, 255)
            likes.TextSize = 18
            likes.Text = "‚ù§ 9.5M"
            likes.Parent = tiktok_frame
            
            local comments = Instance.new("TextLabel")
            comments.Size = UDim2.new(0.4, 0, 0.2, 0)
            comments.Position = UDim2.new(0.55, 0, 0.4, 0)
            comments.BackgroundTransparency = 0.7
            comments.BackgroundColor3 = Color3.fromRGB(50, 150, 255)
            comments.Font = Enum.Font.GothamBold
            comments.TextColor3 = Color3.fromRGB(255, 255, 255)
            comments.TextSize = 18
            comments.Text = "üí¨ 2.4M"
            comments.Parent = tiktok_frame
            
            gui.Parent = character
            ParticleEffects.tiktok_gui = gui
            
            -- Create filter effect
            local filter = Instance.new("ColorCorrectionEffect")
            filter.Brightness = 0.1
            filter.Contrast = 0.1
            filter.Saturation = 0.2
            filter.TintColor = Color3.fromRGB(255, 200, 180)
            filter.Parent = game:GetService("Lighting")
            ParticleEffects.tiktok_filter = filter
            
            print("  - Virtual phone UI overlay appears")
            print("  - Digital filters change with dance movements")
            print("  - Floating like/comment icons appear randomly")
        else
            print("üì± Removing NEO TIKTOK AR effects")
            
            -- Remove TikTok UI
            if ParticleEffects.tiktok_gui then
                ParticleEffects.tiktok_gui:Destroy()
                ParticleEffects.tiktok_gui = nil
            end
            
            -- Remove filter effect
            if ParticleEffects.tiktok_filter then
                ParticleEffects.tiktok_filter:Destroy()
                ParticleEffects.tiktok_filter = nil
            end
        end
    end,
    
    -- QUANTUM SHUFFLE special effects
    quantum = function(character, action)
        if action == "start" then
            print("‚ö° Adding quantum effects for QUANTUM SHUFFLE")
            
            -- Create teleportation effect with transparency flickering
            local function setupQuantumEffect()
                ParticleEffects.quantum_thread = coroutine.create(function()
                    local enabled = true
                    while enabled do
                        if not ParticleEffects.quantum_thread then
                            break
                        end
                        
                        -- Make character flicker in and out
                        for _, part in pairs(character:GetDescendants()) do
                            if part:IsA("BasePart") and part.Transparency < 0.9 then
                                part.Transparency = 0.7
                            end
                        end
                        wait(0.1)
                        
                        -- Create teleport effect
                        local teleport_effect = Instance.new("Part")
                        teleport_effect.Size = Vector3.new(1, 1, 1)
                        teleport_effect.CFrame = character:GetPrimaryPartCFrame()
                        teleport_effect.Anchored = true
                        teleport_effect.CanCollide = false
                        teleport_effect.Transparency = 0.5
                        teleport_effect.Material = Enum.Material.Neon
                        teleport_effect.Color = Color3.fromRGB(0, 255, 100)
                        teleport_effect.Shape = Enum.PartType.Ball
                        teleport_effect.Parent = workspace
                        
                        -- Create shock wave
                        local shockwave = Instance.new("Part")
                        shockwave.Size = Vector3.new(1, 0.2, 1)
                        shockwave.CFrame = character:GetPrimaryPartCFrame() * CFrame.new(0, -3, 0)
                        shockwave.Anchored = true
                        shockwave.CanCollide = false
                        shockwave.Transparency = 0.5
                        shockwave.Material = Enum.Material.Neon
                        shockwave.Color = Color3.fromRGB(0, 255, 100)
                        shockwave.Shape = Enum.PartType.Cylinder
                        shockwave.Parent = workspace
                        
                        -- Grow and fade shockwave
                        for i = 1, 10 do
                            shockwave.Size = Vector3.new(i*0.7, 0.1, i*0.7)
                            shockwave.Transparency = 0.5 + (i * 0.05)
                            wait(0.02)
                        end
                        
                        -- Remove effect objects
                        teleport_effect:Destroy()
                        shockwave:Destroy()
                        
                        -- Return character to normal
                        for _, part in pairs(character:GetDescendants()) do
                            if part:IsA("BasePart") and part.Transparency >= 0.5 then
                                part.Transparency = 0
                            end
                        end
                        wait(0.3)
                    end
                end)
                coroutine.resume(ParticleEffects.quantum_thread)
            end
            
            setupQuantumEffect()
            
            print("  - Character briefly phases in and out of visibility")
            print("  - Teleportation effects during sharp movements")
            print("  - Reality distortion visual when dancing")
        else
            print("‚ö° Removing QUANTUM SHUFFLE quantum effects")
            
            -- Stop quantum thread
            if ParticleEffects.quantum_thread then
                ParticleEffects.quantum_thread = nil
            end
            
            -- Return character to normal visibility
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") and part.Transparency >= 0.5 then
                    part.Transparency = 0
                end
            end
        end
    end
}

