-- Neo TikTok button script
local function neoTikTokButton()
    local script = Instance.new('LocalScript')
    script.Parent = TextButton_13
    
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local hum = character:WaitForChild("Humanoid")
    local emote = hum:LoadAnimation(script.Parent.Emote)
    local playing = false
    
    script.Parent.MouseButton1Click:connect(function()
        if not playing then
            script.Parent.Sound.Playing = true
            emote:Play()
            -- Increase speed for TikTok style
            emote:AdjustSpeed(1.5)
            hum.WalkSpeed = 0
            hum.JumpPower = 0
            playing = true
            
            -- Neo TikTok 2025 AR effect
            local billboardGui = Instance.new("BillboardGui")
            billboardGui.Adornee = character.Head
            billboardGui.Size = UDim2.new(3, 0, 0.5, 0)
            billboardGui.StudsOffset = Vector3.new(0, 2, 0)
            billboardGui.AlwaysOnTop = true
            
            local textLabel = Instance.new("TextLabel")
            textLabel.BackgroundTransparency = 1
            textLabel.Size = UDim2.new(1, 0, 1, 0)
            textLabel.Font = Enum.Font.GothamBold
            textLabel.TextScaled = true
            textLabel.TextColor3 = Color3.fromRGB(255, 100, 50)
            textLabel.Text = "@TikTok2025"
            textLabel.Parent = billboardGui
            
            billboardGui.Parent = character
            
            -- Store for cleanup
            script.billboardGui = billboardGui
        else
            script.Parent.Sound.Playing = false
            emote:Stop()
            hum.WalkSpeed = 16
            hum.JumpPower = 50
            playing = false
            
            -- Remove billboard
            if script.billboardGui then
                script.billboardGui:Destroy()
                script.billboardGui = nil
            end
        end
    end)
end
coroutine.resume(coroutine.create(neoTikTokButton))

-- Quantum Shuffle button script
local function quantumShuffleButton()
    local script = Instance.new('LocalScript')
    script.Parent = TextButton_14
    
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local hum = character:WaitForChild("Humanoid")
    local emote = hum:LoadAnimation(script.Parent.Emote)
    local playing = false
    
    script.Parent.MouseButton1Click:connect(function()
        if not playing then
            script.Parent.Sound.Playing = true
            emote:Play()
            hum.WalkSpeed = 0
            hum.JumpPower = 0
            playing = true
            
            -- 2025 quantum teleport effect
            local function createTeleportEffect()
                local oldPos = character.HumanoidRootPart.Position
                
                -- Small random offset teleport
                task.delay(0.1, function()
                    local randomOffset = Vector3.new(math.random(-2, 2), 0, math.random(-2, 2))
                    character.HumanoidRootPart.Position = oldPos + randomOffset
                    
                    -- Teleport effect
                    local part = Instance.new("Part")
                    part.Shape = Enum.PartType.Ball
                    part.Material = Enum.Material.Neon
                    part.Size = Vector3.new(1, 1, 1)
                    part.Position = oldPos
                    part.Anchored = true
                    part.CanCollide = false
                    part.Color = Color3.fromRGB(0, 255, 100)
                    part.Transparency = 0.5
                    part.Parent = workspace
                    
                    -- Fade out
                    for i = 0.5, 1, 0.1 do
                        task.wait(0.05)
                        part.Transparency = i
                        part.Size = part.Size - Vector3.new(0.1, 0.1, 0.1)
                    end
                    part:Destroy()
                end)
            end
            
            -- Create the teleport effect every 0.5 seconds
            local teleportLoop = task.spawn(function()
                while playing do
                    createTeleportEffect()
                    task.wait(0.5)
                end
            end)
            
            -- Store for cleanup
            script.teleportLoop = teleportLoop
        else
            script.Parent.Sound.Playing = false
            emote:Stop()
            hum.WalkSpeed = 16
            hum.JumpPower = 50
            playing = false
            
            -- Stop teleport effect
            if script.teleportLoop then
                task.cancel(script.teleportLoop)
                script.teleportLoop = nil
            end
        end
    end)
end
coroutine.resume(coroutine.create(quantumShuffleButton))

-- Handle character respawning
local function setupRespawnHandler()
    local player = game.Players.LocalPlayer
    
    player.CharacterAdded:Connect(function(newCharacter)
        -- Reset all animations when character respawns
        if Frame and Frame.Visible then
            -- Re-initialize all buttons when character respawns
            toggleMainGUI()
            danceRobloxButton()
            helloButton()
            fortniteDanceButton()
            chickenDanceButton()
            robotDanceButton()
            monkeyDanceButton()
            breakdanceButton()
            laughEmoteButton()
            twerk2025Button()
            futureGriddyButton()
            holoDance2025Button()
            neoTikTokButton()
            quantumShuffleButton()
        end
    end)
end
coroutine.resume(coroutine.create(setupRespawnHandler))

-- Return the ScreenGui so it can be used
return Danceguinotvirusandattack
