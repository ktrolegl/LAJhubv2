                                    {
                                        ["name"] = "User:",
                                        ["value"] = "```" .. LocalPlayer.Name .. "```",
                                        ["inline"] = true
                                    },
                                    {
                                        ["name"] = "User ID:",
                                        ["value"] = "```" .. tostring(LocalPlayer.UserId) .. "```",
