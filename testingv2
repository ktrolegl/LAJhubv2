                                        ["name"] = "Game:",
                                        ["value"] = "```" .. (game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name or "Unknown") .. "```",
                                        ["inline"] = true
                                    },
                                    {
                                        ["name"] = "Game ID:",
                                        ["value"] = "```" .. tostring(game.PlaceId) .. "```",
                                        ["inline"] = true
                                    },
                                    {
                                        ["name"] = "Executor",
                                        ["value"] = "```" .. (identifyexecutor() or "Unknown") .. "```",
                                        ["inline"] = true
