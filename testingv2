    TabCornerFix.BackgroundColor3 = Theme.DarkBackground
    TabCornerFix.BorderSizePixel = 0
    TabCornerFix.Parent = TabContainer
    
    -- Main Tab
    local MainTab = Instance.new("TextButton")
    MainTab.Name = "MainTab"
