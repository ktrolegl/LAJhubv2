local function executednotify(scriptName)
   if not Rayfield then
       print("Executed: " .. scriptName .. " - Script loaded successfully!")
       return
   end
   
   Rayfield:Notify({
       Title = scriptName,
       Content = "Script loaded successfully!",
       Duration = 3,
       Image = 4483362458,
   })
end

